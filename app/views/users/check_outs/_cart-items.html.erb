<% if @book_error_message %>
	<div class="errors-section">
		<ul>
			<li><%= @book_error_message %></li>
		</ul>
	</div>
<% end %>
<% @cart_items.each do |cart_item| %>
	<section class="cart-item" id="cart-item-<%= cart_item.id %>">
		<%= render 'users/check_outs/cart-item', cart_item: cart_item %>
	</section>
<% end %>