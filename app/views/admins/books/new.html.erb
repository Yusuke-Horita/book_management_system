<h1>書籍登録</h1>
<ul>
	<% if @exists_error %>
		<li><%= @exists_error %></li>
	<% end %>
	<% if @book.errors.any? %>
		<% @book.errors.full_messages.each do |message| %>
			<li><%= message %></li>
		<% end %>
	<% end %>
</ul>
<%= form_with model: [:admins, @book], local: true do |f| %>
	<p><%= f.label :code, "Code" %></p>
	<p><%= f.text_field :code %></p>
	<p><%= f.label :title, "Title" %></p>
	<p><%= f.text_field :title %></p>
	<p><%= f.label :stock, "Stock" %></p>
	<p><%= f.text_field :stock %></p>
	<p><%= f.submit %></p>
<% end %>
